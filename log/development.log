  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Feb 14, 2017"], ["review_text", "Then, Now. Then, Now. Then, Now. A good read about a unique house that two women wanted to live in. But wait, the house is not the central attraction, it is the women's relationships to a couple of characters. A mystery that kept the pages turning."], ["platform", "Goodreads"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.305197"], ["updated_at", "2017-08-23 17:29:47.305197"]]
