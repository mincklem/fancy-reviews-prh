  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Apr 06, 2017"], ["review_text", "Se deja leer es lo mejor que se me ocurre."], ["platform", "Goodreads"], ["star_rating", 1], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.257387"], ["updated_at", "2017-08-23 17:29:47.257387"]]
