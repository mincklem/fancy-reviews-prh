  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Jul 09, 2017"], ["review_text", "Um thriller psicológico que se distingue de muitos outros em originalidade: todo o mistério e descrição da casa, o início de cada capítulo com uma das perguntas do famoso questionário... gostei! O fim, deixou algo a desejar... "], ["platform", "Goodreads"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:11:16.960228"], ["updated_at", "2017-08-23 17:11:16.960228"]]
