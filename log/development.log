  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "May 16, 2017"], ["review_text", "Procitana u dva navrata. Svidio mi se i nacin i stil pripovijedanja. Ideja dobra te tjera na citanje. Preporuka od mene za lagani triler (vise krimic).."], ["platform", "Goodreads"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:46.884526"], ["updated_at", "2017-08-23 17:29:46.884526"]]
