  [1m[36mSQL (0.2ms)[0m  [1mINSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)[0m  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "July 8, 2017"], ["review_text", "I have become a fan of this writing style and here it is done to perfection. A big fan also of Mysteries, I was able to solve this one but closer to its end than usual. Love that the title's meaning was it's own little mystery."], ["platform", "Amazon"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.921728"], ["updated_at", "2017-08-23 17:29:47.921728"]]
