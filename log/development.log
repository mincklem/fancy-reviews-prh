  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "July 12, 2017"], ["review_text", "I loved the intelligent writing, humor, magic and history in this novel.  Oberon is hilarious and Atticus' lawyers are great.  The editing was spot on!  Great, great book!  Highly recommend!"], ["platform", "Amazon"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:48.372171"], ["updated_at", "2017-08-23 18:11:48.372171"]]
