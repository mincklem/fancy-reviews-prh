  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Mar 19, 2017"], ["review_text", "4,25/5 Very solid writing, real people, highly relevant, flawless story telling. Just lacked a touch of originality and wow-factor to climb even higher."], ["platform", "Goodreads"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:20.986493"], ["updated_at", "2017-08-23 18:46:20.986493"]]
