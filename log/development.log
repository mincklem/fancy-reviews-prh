  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Sep 10, 2016"], ["review_text", "A pretty simple story ultimately but had me so invested from the start. One of those books where you're curious how the characters end up afterwards."], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:22.049791"], ["updated_at", "2017-08-23 18:46:22.049791"]]
