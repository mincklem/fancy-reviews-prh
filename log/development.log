  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Feb 05, 2017"], ["review_text", "While I thought this was a unique storyline amp; liked the different POV, it was a hodgepodge mess that was a jumbled mess towards the end. Like it was going for shock value switching the story quickly but it kind of fell flat for me. "], ["platform", "Goodreads"], ["star_rating", 2], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:11:16.896528"], ["updated_at", "2017-08-23 17:11:16.896528"]]
