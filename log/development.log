  [1m[35mSQL (0.1ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "Aug 08, 2014"], ["review_text", "Not too shabby. I had some serious issues with how women were represented, and it made me think about how female authors in this genre (and MANY others) portray men. RTC. "], ["platform", "Goodreads"], ["star_rating", 3], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:47.304341"], ["updated_at", "2017-08-23 18:11:47.304341"]]
