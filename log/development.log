  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Jan 24, 2017"], ["review_text", " special thank you to Random House and NetGalley for an ARC in exchange for an honest review. Review to follow. "], ["platform", "Goodreads"], ["star_rating", 3], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.337525"], ["updated_at", "2017-08-23 17:29:47.337525"]]
