  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "April 16, 2017"], ["review_text", "Delivered on time and with out incident.The novel itself is well written with engaging characters that will lead the reader on a wonderful adventure."], ["platform", "Amazon"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:48.509527"], ["updated_at", "2017-08-23 18:11:48.509527"]]
