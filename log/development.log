  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "March 26, 2017"], ["review_text", "It's the best thriller I've read in years.  You won't be disappointed.  Lots of twists and turns.  Believable characters.  If you liked \"Gone Girl\" or \"Girl on a Train\" this is the book for you."], ["platform", "Amazon"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:49.085499"], ["updated_at", "2017-08-23 17:29:49.085499"]]
