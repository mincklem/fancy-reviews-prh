  [1m[36mSQL (0.1ms)[0m  [1mINSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)[0m  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Sep 16, 2016"], ["review_text", "Oh how I loved this book! I'm very excited to see it's the new Oprah Book Club pick. Hopefully more people will read this fantastic novel!"], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:20.968454"], ["updated_at", "2017-08-23 18:46:20.968454"]]
