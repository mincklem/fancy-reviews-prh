  [1m[35mSQL (0.1ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Jul 26, 2017"], ["review_text", "Quite a page turner. Great story of the challenges of making it , or not , as an immigrant in America ."], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:21.919503"], ["updated_at", "2017-08-23 18:46:21.919503"]]
