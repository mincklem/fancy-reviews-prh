  [1m[36mSQL (0.2ms)[0m  [1mINSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)[0m  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "May 08, 2017"], ["review_text", "This is a very unique thrller. I loved it. You think you know where this is heading but you are wrong. I enjoyed this one very much because it surprised me."], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.254964"], ["updated_at", "2017-08-23 17:29:47.254964"]]
