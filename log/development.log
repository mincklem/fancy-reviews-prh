  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "March 28, 2016"], ["review_text", "This book is hilarious! It's also fun reading it because I'm from the area and it's a great kick reading about places I know and love.The main character is refreshing and though he can be a bit immature for his age, he acknowledges his own flaws and gives enough back story that you feel like you know where he's coming from.Great book series!"], ["platform", "Amazon"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:48.475159"], ["updated_at", "2017-08-23 18:11:48.475159"]]
