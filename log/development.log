  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "April 6, 2017"], ["review_text", "What a fun read. I love magic druids witches and Irish fairy takes. This story didn't disappoint. Love the first person narrative and touches of humor. Was introduced to the series through my book club and plan on continuing with the rest of the series."], ["platform", "Amazon"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:48.447364"], ["updated_at", "2017-08-23 18:11:48.447364"]]
