  [1m[35mSQL (0.1ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "Jun 11, 2017"], ["review_text", "Fără îndoială, ,,The Girl Before” e cel mai bun thriller psihologic pe care l-am citit până acum :) O poveste încâlcită, personaje misterioase, o scriitură interesantă, happy-end. Ce-ți poți dori mai mult? :D "], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.449568"], ["updated_at", "2017-08-23 17:29:47.449568"]]
