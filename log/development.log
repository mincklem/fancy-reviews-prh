  [1m[35mSQL (0.1ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Aug 23, 2016"], ["review_text", "One of my favorite books this year. Devastatingly beautiful writing and a story that rips your heart out over and over again. "], ["platform", "Goodreads"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:21.102690"], ["updated_at", "2017-08-23 18:46:21.102690"]]
