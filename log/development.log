  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "28016509"], ["title", "The Girl Before"], ["author", "J.P. Delaney"], ["date", "August 9, 2017"], ["review_text", "I am not a fan of books where every other chapter is someone else, and it took me a little while to keep the girls in the correct time and place, however, it moved quickly to the mysteries within this house. It was a quick read and I liked it."], ["platform", "Amazon"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1485972587m/28016509.jpg"], ["created_at", "2017-08-23 17:29:47.914096"], ["updated_at", "2017-08-23 17:29:47.914096"]]
