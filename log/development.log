  [1m[35mSQL (0.2ms)[0m  INSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)  [["isbn", "35259724"], ["title", "Behold the Dreamers"], ["author", "Imbolo Mbue"], ["date", "Mar 24, 2017"], ["review_text", "Loved the characters and the way the story weaved between two worlds within one city but the ending is maddening to me. I'm left feeling annoyed at two of my favorite characters. "], ["platform", "Goodreads"], ["star_rating", 4], ["img", "https://images.gr-assets.com/books/1498483394m/35259724.jpg"], ["created_at", "2017-08-23 18:46:21.821729"], ["updated_at", "2017-08-23 18:46:21.821729"]]
