  [1m[36mSQL (0.2ms)[0m  [1mINSERT INTO "reviews" ("isbn", "title", "author", "date", "review_text", "platform", "star_rating", "img", "created_at", "updated_at") VALUES (?, ?, ?, ?, ?, ?, ?, ?, ?, ?)[0m  [["isbn", "9533378"], ["title", "Hounded (The Iron Druid Chronicles, #1)"], ["author", "Kevin Hearne"], ["date", "February 25, 2017"], ["review_text", "Good read. A Druid who is 21 and living in Arizona. Has problems when an old for finds him and wants a magic sword. The foe is very old because the druid is 21 centuries old he looks 21 years old."], ["platform", "Amazon"], ["star_rating", 5], ["img", "https://images.gr-assets.com/books/1327883998m/9533378.jpg"], ["created_at", "2017-08-23 18:11:48.369761"], ["updated_at", "2017-08-23 18:11:48.369761"]]
